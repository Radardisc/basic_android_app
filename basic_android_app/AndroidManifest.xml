<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.locomas.android"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
 
    <uses-permission android:name="android.permission.INTERNET" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.locomas.android.DatesActivity"
            android:label="@string/app_name" >
            <intent-filter> 
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.locomas.android.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter> 
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
        <activity
            android:name="com.locomas.android.InitialActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.locomas.android.MoreActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.locomas.android.SignUpActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.locomas.android.RetailerSignUpActivity"
            android:label="@string/app_name" >
        </activity>
        
        
        
        
        
    </application>

</manifest>
